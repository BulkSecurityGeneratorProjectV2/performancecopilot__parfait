<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <parent>
    <artifactId>aconex</artifactId>
    <groupId>com.aconex</groupId>
    <version>1.1.9</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.aconex</groupId>
  <artifactId>aconex-pcp-bridge</artifactId>
  <name>aconex-pcp-bridge</name>
  <version>1.4.5-SNAPSHOT</version>
  <description>Bridges Java-&gt;Performance Co-pilot(PCP) monitoring system</description>
  <inceptionYear>2007</inceptionYear>
  <scm>
    <connection>scm:svn:https://svn.aconex.com/svn/engr/aconex-pcp-bridge</connection>
  </scm>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-surefire-plugin</artifactId>
        <configuration>
          <forkMode>always</forkMode>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>com.aconex</groupId>
      <artifactId>aconex-assert</artifactId>
      <version>1.1</version>
    </dependency>
    <dependency>
      <groupId>log4j</groupId>
      <artifactId>log4j</artifactId>
      <version>1.2.14</version>
    </dependency>
    <dependency>
      <groupId>commons-lang</groupId>
      <artifactId>commons-lang</artifactId>
      <version>2.4</version>
    </dependency>
    <dependency>
      <groupId>org.springframework</groupId>
      <artifactId>spring</artifactId>
      <version>2.5.5</version>
    </dependency>
    <dependency>
      <groupId>commons-io</groupId>
      <artifactId>commons-io</artifactId>
      <version>1.4</version>
    </dependency>
  </dependencies>
</project>
